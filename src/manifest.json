{
  "manifest_version": 2,
  "icons": {
    "128": "images/icon.png"
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "html/popup.html"
  },
  "background": {
    "scripts": ["js/background.js"]
  },
  "permissions": [
    "*://*.github.com/users/*"
  ],
  "name": "<%= package.name %>",
  "version": "<%= package.version %>",
  "description": "<%= package.description %>"
}
